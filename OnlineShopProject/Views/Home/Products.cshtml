@{
    ViewData["Title"] = "Our Products";
    Layout = "_Layout";

}



<main>

    <h1>Our Products</h1>

    <div class="container">
        @foreach (var item in Model)
         {
           
                <h2>@item.ProductName</h2>
                <img src="~/images/@item.Image" alt="@item.ProductName" />
                <p>@item.PrDescription</p>

         }
    </div>

</main>

<partial name="_Footer" />
